<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aek.ebey.repair.mapper.RepRepairCheckMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.aek.ebey.repair.model.RepRepairCheck">
		<id column="id" property="id" />
		<result column="apply_id" property="applyId" />
		<result column="repair_check_id" property="repairCheckId" />
		<result column="repair_check_name" property="repairCheckName" />
		<result column="repair_check_time" property="repairCheckTime" />
		<result column="repair_attitude" property="repairAttitude" />
		<result column="response_speed" property="responseSpeed" />
		<result column="repair_quality" property="repairQuality" />
		<result column="remarks" property="remarks" />
		<result column="del_flag" property="delFlag" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, apply_id AS applyId, repair_check_id AS repairCheckId, repair_check_name AS repairCheckName, repair_check_time AS repairCheckTime, repair_attitude AS repairAttitude, response_speed AS responseSpeed, repair_quality AS repairQuality, remarks, del_flag AS delFlag
    </sql>
</mapper>
