<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aek.ebey.repair.mapper.RepairDictionaryMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.aek.ebey.repair.model.RepairDictionary">
		<id column="id" property="id" />
		<result column="type_key" property="typeKey" />
		<result column="key_id" property="keyId" />
		<result column="name" property="name" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, type_key AS typeKey, key AS keyId, name
    </sql>
</mapper>
