<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aek.ebey.repair.mapper.RepRepairBillPartsMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.aek.ebey.repair.model.RepRepairBillParts">
		<id column="id" property="id" />
		<result column="bill_id" property="billId" />
		<result column="part_name" property="partName" />
		<result column="part_spec" property="partSpec" />
		<result column="part_produce" property="partProduce" />
		<result column="part_price" property="partPrice" />
		<result column="unit" property="unit" />
		<result column="num" property="num" />
		<result column="create_time" property="createTime" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, 
        bill_id AS billId, 
        part_name AS partName,
		part_spec AS partSpec,
		part_produce AS partProduce,
		part_price AS partPrice,
		unit AS unit,
		num AS num,
		create_time AS createTime
    </sql>
    
</mapper>
