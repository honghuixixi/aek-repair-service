<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aek.ebey.repair.mapper.RepRepairMessageMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.aek.ebey.repair.model.RepRepairMessage">
		<id column="id" property="id" />
		<result column="module_id" property="moduleId" />
		<result column="message_content" property="messageContent" />
		<result column="message_time" property="messageTime" />
		<result column="remarks" property="remarks" />
		<result column="del_flag" property="delFlag" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, module_id AS moduleId, message_content AS messageContent, message_time AS messageTime, remarks AS remarks, del_flag AS delFlag
    </sql>
</mapper>
